{
  "name": "visitor",
  "version": "0.0.0",
  "description": "walk and transform ASTs with references to parent nodes",
  "main": "index.js",
  "keywords": [
    "ast",
    "walk",
    "tree",
    "syntax",
    "source",
    "esprima",
    "transform",
    "traversal"
  ],
  "dependencies": {
    "esprima": "1.0.2",
    "underscore": "1.4.4"
  },
  "devDependencies": {
    "tap": "~0.4.2",
    "tape": "~1.0.2",
    "escodegen": "~0.0.22"
  },
  "scripts": {
